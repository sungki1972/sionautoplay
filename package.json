{
  "name": "youtube-scheduler",
  "version": "1.0.0",
  "description": "YouTube video scheduler with date-based auto play",
  "main": "server/index.js",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "node server/index.js",
    "client": "cd client && npm run dev",
    "install-all": "npm install && cd client && npm install",
    "build": "cd client && rm -rf dist node_modules && npm install && npm run build",
    "start": "npm run build && node server/index.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "body-parser": "^1.20.2",
    "@supabase/supabase-js": "^2.38.4"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "youtube",
    "scheduler",
    "video"
  ],
  "author": "",
  "license": "ISC"
}

